        "dev": "node server-docker.js",
        "start": "NODE_ENV=production node server-docker.js",
        "gcloud:submit-build": "gcloud builds submit --tag gcr.io/contractors-f1d51/mq --project contractors-f1d51 --timeout=5h",
        "gcloud:deploy": "gcloud run deploy mq --region=us-central1 --image=gcr.io/contractors-f1d51/mq --project=contractors-f1d51 --platform=managed --allow-unauthenticated --memory=2Gi --cpu=4 --port=3000 --no-use-http2"
