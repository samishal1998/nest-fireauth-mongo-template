{
	"name": "knot-api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build:prisma": "yarn prisma format && yarn prisma generate",
		"prebuild": "rimraf dist && yarn build:prisma",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"gen:api-client": "export TS_POST_PROCESS_FILE=\"/usr/local/bin/prettier --write\" && openapi-generator generate -g typescript-axios --additional-properties=useSingleRequestParameter=true,withSeparateModelsAndApi=true,enablePostProcessFile=true,modelPackage=models,apiPackage=api --remove-operation-id-prefix -i docs.json -o generated/api-client",
		"gcloud:submit-build": "gcloud builds submit --tag gcr.io/knot-k6789/knot --project knot-k6789 --timeout=5h",
		"gcloud:deploy": "gcloud run deploy knot --region=us-central1 --image=gcr.io/knot-k6789/knot --project=knot-k6789 --platform=managed --allow-unauthenticated --memory=2Gi --cpu=4 --port=3000 --no-use-http2"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/mongoose": "^9.2.1",
		"@nestjs/passport": "^8.2.2",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-socket.io": "^9.0.11",
		"@nestjs/websockets": "^9.0.11",
		"@prisma/client": "^4.7.1",
		"ejs": "^3.1.8",
		"firebase-admin": "^11.0.0",
		"mongodb": "^4.11.0",
		"mongoose": "^6.7.1",
		"nestjs-prisma": "^0.19.0",
		"passport": "^0.6.0",
		"passport-firebase-jwt": "^1.2.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/swagger": "^6.0.5",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.5.0",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.0.0",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "28.0.3",
		"prettier": "^2.3.2",
		"prisma": "^4.7.1",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.1",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.0.0",
		"typescript": "^4.3.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
